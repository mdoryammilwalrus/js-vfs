{
  "name": "js-vfs",
  "version": "1.0.0",
  "description": "JavaScript read-only virtual file system for local files",
  "type": "module",
  "bin": {
    "js-vfs": "src/bin.js"
  },
  "scripts": {
    "docs:dev": "retype watch",
    "docs:build": "yarn jsdoc && retype build",
    "jsdoc": "jsdoc2md --template docs/API.hbs --files src/client/*.js > docs/API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdoryammilwalrus/js-vfs.git"
  },
  "author": "mdoryammilwalrus",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/mdoryammilwalrus/js-vfs/issues"
  },
  "homepage": "https://github.com/mdoryammilwalrus/js-vfs#readme",
  "dependencies": {
    "commander": "^10.0.1",
    "jszip": "^3.10.1",
    "npmlog": "^7.0.1"
  },
  "devDependencies": {
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "retypeapp": "^2.4.0"
  }
}
